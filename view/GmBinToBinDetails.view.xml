<mvc:View class="sapUiSizeCozy" xmlns:mvc="sap.ui.core.mvc" controllerName="smud.org.wmLM01.controller.GmBinToBinDetails" xmlns="sap.m"
	xmlns:form="sap.ui.layout.form">
	<!-- 
						"Matnr" : "",
					    "Maktg" : "",
					    "Meins" : "",
					    "Werks" : "",
					    "Lgort" : "",
					    "Lgnum" : "",
					    "Anfme" : "",
					    "Vltyp" : "",
					    "Vlpla" : "",
					    "Nltyp" : "",
					    "Nlpla" : ""
	-->
	<Page showHeader="true" title="Bin to Bin Move" showFooter="true" showNavButton="false">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:title/>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Warehouse" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpWarehouse" liveChange="uppercaseNoSpecial" value="{/Lgnum}" valueLiveUpdate="true" type="Text" editable="false"
										showValueHelp="false" enabled="true" visible="true" width="75px" valueHelpOnly="false" required="false" valueStateText="Invalid entry"
										maxLength="3"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Plant" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpPlant" value="{/Werks}" valueLiveUpdate="true" type="Text" showValueHelp="false" editable="false" visible="true" width="75px"
										valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="4"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Stor. Location" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpStorageLocation" value="{/Lgort}" valueLiveUpdate="true" type="Text" showValueHelp="false" editable="false" visible="true"
										width="75px" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="4"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Material Number" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
										visible="true" displayInline="false">
										<items>
											<Input id="inpMaterialNumber" value="{/Matnr}" valueLiveUpdate="true" type="Number" showValueHelp="false" editable="false" visible="true"
												width="150px" valueHelpOnly="false" required="false" valueStateText="Invalid entry"/>
											<!--	<Button id="btnMaterialNumber" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true" iconDensityAware="false"
												press="scan"/>-->
										</items>
										<layoutData/>
									</HBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Material Description" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpMaterialDescription" value="{/Maktg}" valueLiveUpdate="true" type="Text" showValueHelp="false" editable="false" visible="true"
										width="350px" valueHelpOnly="false" required="false" valueStateText="Invalid entry"/>
								</form:fields>
							</form:FormElement>
							<!--<form:FormElement visible="true">
								<form:label>
									<Label text="Move Type" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpMovementType" value="{/Meins}" valueLiveUpdate="true" type="Text" showValueHelp="false" editable="true" visible="true" width="50px" valueHelpOnly="false"
										required="false" valueStateText="Invalid entry" maxLength="3"/>
							</form:fields>
							</form:FormElement>-->
							<form:FormElement visible="true">
								<form:label>
									<Label text="UOM" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpUOM" liveChange="uppercaseNoSpecial" value="{/Meins}" valueLiveUpdate="true" type="Text" editable="false"
										showValueHelp="false" enabled="true" visible="true" width="75px" valueHelpOnly="false" required="false" valueStateText="Invalid entry"
										maxLength="5"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Source Ty" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpSourceType" liveChange="uppercaseNoSpecial" value="{/Vltyp}" type="Text" showValueHelp="false" editable="false" visible="true"
										width="125px" valueHelpOnly="false" required="true" valueStateText="Invalid entry" maxLength="3"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Source Bin" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpSourceBin" liveChange="uppercaseNoSpecial" value="{/Vlpla}" type="Text" showValueHelp="false" editable="false" visible="true"
										width="125px" valueHelpOnly="false" required="true" valueStateText="Invalid entry" maxLength="10"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Quantity" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpQuanitity" type="Number" value="{/Anfme}" valueLiveUpdate="true" showValueHelp="false" enabled="true" visible="true"
										width="125px" valueHelpOnly="false" required="true" valueStateText="Invalid entry"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Dest Bin" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
										visible="true" displayInline="false">
										<items>
											<Input id="inpDestinationBin" liveChange="uppercaseNoSpecial" change="getDestStorageType" value="{/Nlpla}" valueLiveUpdate="true"
												type="Text" showValueHelp="false" enabled="true" visible="true" width="125px" valueHelpOnly="false" required="true"
												valueStateText="Invalid entry" maxLength="10"/>
											<Button id="btnDestinationBin" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true"
												iconDensityAware="false" press="scan"/>
										</items>
										<layoutData/>
									</HBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Dest Type" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<ComboBox id="inpDestinationType" value="{/Nltyp}" editable="true" enabled="true" visible="true" width="150px" valueState="None"
										maxWidth="100%"></ComboBox>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
				<content>
					<ToolbarSpacer width=""/>
					<Button text="Save" type="Emphasized" width="auto" enabled="true" visible="true" iconDensityAware="false" press="saveBinToBin"/>
				</content>
			</OverflowToolbar>
		</footer>
		<headerContent/>
		<subHeader/>
		<customHeader/>
	</Page>
</mvc:View>