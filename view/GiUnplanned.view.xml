<mvc:View class="sapUiSizeCozy" xmlns:mvc="sap.ui.core.mvc" controllerName="smud.org.wmLM01.controller.GiUnplanned" xmlns="sap.m"
	xmlns:form="sap.ui.layout.form">
	<Page showHeader="true" title="GI Unplanned" showFooter="true" showNavButton="false">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:title/>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="WH/Plant/SLoc" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
										visible="true" displayInline="false">
										<items>
											<Input id="inpWarehouse" liveChange="uppercaseNoSpecial" value="{/items/0/Lgnum}" valueLiveUpdate="true" type="Text" showValueHelp="false"
												enabled="true" visible="true" width="75px" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="3"/>
											<Input id="inpPlant" value="{/items/0/Werks}" valueLiveUpdate="true" type="Text" showValueHelp="false" editable="false" visible="true"
												width="75px" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="4"/>
											<Input id="inpStorageLocation" value="{/items/0/Lgort}" valueLiveUpdate="true" type="Text" showValueHelp="false" editable="false"
												visible="true" width="75px" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="4"/>
										</items>
										<layoutData/>
									</HBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Move Type" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpMovementType" value="{/items/0/Bwart}" valueLiveUpdate="true" type="Text" showValueHelp="false" editable="false"
										visible="true" width="50px" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="3"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Work Order" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
										visible="true" displayInline="false">
										<items>
											<Input id="inpWorkOrder" value="{/items/0/Aufnr}" valueLiveUpdate="true" type="Tel" showValueHelp="false" enabled="true" visible="true"
												width="150px" valueHelpOnly="false" required="false" valueStateText="Invalid entry" liveChange="checkNumber"/>
											<Button id="btnWorkOrder" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true"
												iconDensityAware="false" press="scan"/>
										</items>
										<layoutData/>
									</HBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Recipient" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
										visible="true" displayInline="false">
										<items>
											<Input id="inpRecipient" value="{/items/0/Wempf}" valueLiveUpdate="true" type="Text" enabled="true" visible="true" width="150px"
												valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="12" change="uppercase"/>
											<Button id="btnRecipient" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true"
												iconDensityAware="false" press="scan"/>
										</items>
										<layoutData/>
									</HBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Vendor" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
										visible="true" displayInline="false">
										<items>
											<Input id="inpVendor" value="{/items/0/Lifnr}" valueLiveUpdate="true" type="Text" showValueHelp="false" enabled="true" visible="true"
												width="125px" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="10"/>
											<Button id="btnVendor" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true"
												iconDensityAware="false" press="scan"/>
										</items>
										<layoutData/>
									</HBox>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
				<content>
					<ToolbarSpacer width=""/>
					<Button text="Next" type="Emphasized" width="auto" enabled="true" visible="true" iconDensityAware="false" press="goToUnplannedItem"/>
				</content>
			</OverflowToolbar>
		</footer>
		<headerContent/>
		<subHeader/>
		<customHeader/>
	</Page>
</mvc:View>