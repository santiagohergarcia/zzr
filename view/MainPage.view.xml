<mvc:View class="sapUiSizeCozy" xmlns:mvc="sap.ui.core.mvc" controllerName="smud.org.wmLM01.controller.MainPage" xmlns="sap.m"
	xmlns:form="sap.ui.layout.form">
	<Page showHeader="false" title="Warehouse Processes" showFooter="false" showNavButton="false">
		<content>
			<IconTabBar expandable="false" expanded="true" applyContentPadding="false" backgroundDesign="Solid" upperCase="false" visible="true"
				selectedKey="Ok">
				<items>
					<IconTabFilter id="tabGoodsReceipt" icon="sap-icon://factory" text="GoodsReceipt" count="" design="Vertical" showAll="false"
						textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
						<content>
							<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="10px" justifyContent="Start" renderType="Div"
								visible="true" displayInline="false">
								<items/>
								<layoutData/>
							</HBox>
							<form:Form width="100%" editable="true" visible="true" title="GOODS RECEIPT">
								<form:title/>
								<form:formContainers>
									<form:FormContainer visible="true">
										<form:formElements>
											<form:FormElement visible="true">
												<form:label>
													<Label text="PO Number" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
												</form:label>
												<form:fields>
													<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
														visible="true" displayInline="false">
														<items>
															<Input value="{/grEbeln}" id="inpMainPagePoNumber" type="Tel" showValueHelp="false" enabled="true" visible="true" width="150px"
																valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="10" liveChange="checkNumber"/>
															<Button id="btnMainPagePoNumber" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true"
																iconDensityAware="false" press="scanPoNumber"/>
															<Button text="Go" type="Emphasized" width="50px" enabled="true" visible="true" iconDensityAware="false" press="getOpenItems"/>
														</items>
														<layoutData/>
													</HBox>
												</form:fields>
											</form:FormElement>
										</form:formElements>
										<form:title/>
									</form:FormContainer>
								</form:formContainers>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
							<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="20px" justifyContent="Start" renderType="Div"
								visible="true" displayInline="false">
								<items/>
								<layoutData/>
							</HBox>
							<form:Form width="100%" editable="true" visible="true" title="PRINT GR LABEL">
								<form:title/>
								<form:formContainers>
									<form:FormContainer visible="true">
										<form:formElements>
											<form:FormElement visible="true">
												<form:label>
													<Label text="Warehouse" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
												</form:label>
												<form:fields>
													<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
														visible="true" displayInline="false">
														<items>
															<Input id="inpMainPageWarehouse" value="{/grPrintLgnum}" type="Text" showValueHelp="false" enabled="true" visible="true" width="75px"
																valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="3"/>
														</items>
														<layoutData/>
													</HBox>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<Label text="Mat Doc #" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
												</form:label>
												<form:fields>
													<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
														visible="true" displayInline="false">
														<items>
															<Input id="inpMainPageMaterialDocument" value="{/grPrintMblnr}" type="Tel" showValueHelp="false" enabled="true" visible="true" width="150px"
																valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="10" liveChange="checkNumber"/>
															<Button id="btnMainPageMaterialDocument" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true"
																visible="true" iconDensityAware="false" press="scanForPrint"/>
															<Button text="Go" type="Emphasized" width="50px" enabled="true" visible="true" iconDensityAware="false" press="getPrintItems"/>
														</items>
														<layoutData/>
													</HBox>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<Label text="PO Number" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
												</form:label>
												<form:fields>
													<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
														visible="true" displayInline="false">
														<items>
															<Input id="inpMainPagePoNumberPrint" value="{/grPrintEbeln}" type="Tel" showValueHelp="false" enabled="true" visible="true" width="150px"
																valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="10" liveChange="checkNumber"/>
															<Button id="btnMainPagePoNumberPrint" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true"
																iconDensityAware="false" press="scanForPrint"/>
															<Button text="Go" type="Emphasized" width="50px" enabled="true" visible="true" iconDensityAware="false" press="getPrintItems"/>
														</items>
														<layoutData/>
													</HBox>
												</form:fields>
											</form:FormElement>
										</form:formElements>
										<form:title/>
									</form:FormContainer>
								</form:formContainers>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
							<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
								visible="true" displayInline="false" class="sapUiTinyMargin">
								<items></items>
								<layoutData/>
							</HBox>
						</content>
					</IconTabFilter>
					<IconTabSeparator icon="" iconDensityAware="false" visible="true"/>
					<IconTabFilter id="tabPutaway" icon="sap-icon://basket" text="Putaway" count="" design="Vertical" showAll="false" textDirection="Inherit"
						enabled="true" visible="true" iconDensityAware="false">
						<content>
							<form:Form width="100%" editable="true" visible="true" title="PUTAWAY">
								<form:title/>
								<form:formContainers>
									<form:FormContainer visible="true">
										<form:formElements>
											<form:FormElement visible="true">
												<form:label>
													<Label text="Warehouse" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
												</form:label>
												<form:fields>
													<Input id="inpPutawayWarehouse" value="{/putawayLgnum}" type="Text" showValueHelp="false" enabled="true" visible="true" width="75px"
														valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="3"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<Label text="Material" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
												</form:label>
												<form:fields>
													<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
														visible="true" displayInline="false">
														<items>
															<Input id="inpPutawayMaterial" value="{/putawayMatnr}" type="Text" showValueHelp="true" enabled="true" visible="true" width="175px"
																valueHelpOnly="true" required="false" valueStateText="Invalid entry" maxLength="18" valueHelpRequest="f4PutawayMatnr"/>
															<Button id="btnPutawayMaterial" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true"
																iconDensityAware="false" press="scanMaterial"/>
														</items>
													</HBox>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<Label text="Consignment" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
												</form:label>
												<form:fields>
													<CheckBox id="cbConsignment" text="" selected="{/consignment}" editable="true" enabled="true" visible="true" width="auto"
														textDirection="Inherit"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<Label text="" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="false"/>
												</form:label>
												<form:fields>
													<Button text="Next" type="Emphasized" width="100px" enabled="true" visible="true" iconDensityAware="false" press="goToPutaway"/>
												</form:fields>
											</form:FormElement>
										</form:formElements>
										<form:title/>
									</form:FormContainer>
								</form:formContainers>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
						</content>
					</IconTabFilter>
					<IconTabSeparator icon="" iconDensityAware="false" visible="true"/>
					<IconTabFilter id="tabGoodsIssue" icon="sap-icon://shipping-status" text="Goods Issue" count="" design="Vertical" showAll="false"
						textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
						<content>
							<form:Form width="100%" editable="true" visible="true" title="GOODS ISSUE">
								<form:title/>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
							<VBox alignItems="Center" direction="Column" fitContainer="false" width="auto" height="auto" justifyContent="Center" renderType="Div"
								visible="true" displayInline="false" class="sapUiTinyMargin">
								<items>
									<Button text="Planned" type="Default" width="200px" enabled="true" visible="true" iconDensityAware="false" press="goToGiPlanned"/>
									<Button text="Unplanned" type="Default" width="200px" enabled="true" visible="true" iconDensityAware="false" press="goToGiUnplanned"/>
									<Button text="Replenish" type="Default" width="200px" enabled="true" visible="true" iconDensityAware="false" press="goToGiReplenish"/>
								</items>
							</VBox>
						</content>
					</IconTabFilter>
					<IconTabSeparator icon="" iconDensityAware="false" visible="true"/>
					<IconTabFilter id="tabReturns" icon="sap-icon://repost" text="Returns" count="" design="Vertical" showAll="false" textDirection="Inherit"
						enabled="true" visible="true" iconDensityAware="false">
						<content>
							<form:Form width="100%" editable="true" visible="true" title="RETURNS">
								<form:title/>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
							<VBox alignItems="Center" direction="Column" fitContainer="false" width="auto" height="auto" justifyContent="Center" renderType="Div"
								visible="true" displayInline="false" class="sapUiTinyMargin">
								<items>
									<Button text="GR Returns" type="Default" iconFirst="true" width="200px" enabled="true" visible="true" iconDensityAware="false"
										press="goToReturn"/>
									<Button text="Print Material Label" type="Default" iconFirst="true" width="200px" enabled="true" visible="true" iconDensityAware="false"
										press="goToReturnPrint"/>
								</items>
							</VBox>
						</content>
					</IconTabFilter>
					<IconTabSeparator icon="" iconDensityAware="false" visible="true"/>
					<IconTabFilter id="tabWM" icon="sap-icon://business-one" text="WM" count="" design="Vertical" showAll="false" textDirection="Inherit"
						enabled="true" visible="true" iconDensityAware="false">
						<content>
							<form:Form width="100%" editable="true" visible="true" title="WM PROCESSES">
								<form:title/>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
							<VBox alignItems="Center" direction="Column" fitContainer="false" width="auto" height="auto" justifyContent="Center" renderType="Div"
								visible="true" displayInline="false" class="sapUiTinyMargin">
								<items>
									<Button text="Bin to Bin" type="Default" iconFirst="true" width="200px" enabled="true" visible="true" iconDensityAware="false"
										press="goToGmBinToBin"/>
									<Button text="Stock Overview" type="Default" iconFirst="true" width="200px" enabled="true" visible="true" iconDensityAware="false" press="goToWmStockOverview"/>
									<Button text="Print Bin Label" type="Default" iconFirst="true" width="200px" enabled="true" visible="true" iconDensityAware="false" press="goToWmPrintBinLabel"/>
									<Button text="Inventory" type="Default" iconFirst="true" width="200px" enabled="true" visible="true" iconDensityAware="false"
										press="goToWmInventory"/>
									<Button text="Print Material Label" type="Default" iconFirst="true" width="200px" enabled="true" visible="true" iconDensityAware="false"
										press="goToReturnPrint"/>
								</items>
							</VBox>
						</content>
					</IconTabFilter>
				</items>
				<content/>
			</IconTabBar>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
				<content/>
			</OverflowToolbar>
		</footer>
		<headerContent/>
		<subHeader/>
		<customHeader/>
	</Page>
</mvc:View>