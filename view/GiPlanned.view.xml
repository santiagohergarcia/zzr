<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="smud.org.wmLM01.controller.GiPlanned" xmlns="sap.m" xmlns:form="sap.ui.layout.form">
	<Page showHeader="true" title="Goods Issue Planned" showFooter="true" showNavButton="false">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:title/>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Warehouse" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input value="{/Lgnum}" type="Text" showValueHelp="false" enabled="true" visible="true" width="75px" valueHelpOnly="false" required="false"
										valueStateText="Invalid entry" maxLength="3"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Transfer Order" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
										visible="true" displayInline="false">
										<items>
											<Input id="inpTanum" value="{/Tanum}" type="Tel" showValueHelp="false" enabled="true" visible="true" width="150px" valueHelpOnly="false"
												required="false" valueStateText="Invalid entry" maxLength="10" liveChange="tanumLiveChange"/>
											<Button id="btnTanum" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true"
												iconDensityAware="false" press="scanTanum"/>
										</items>
										<layoutData/>
									</HBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Order#" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input value="{/Benum}" type="Text" showValueHelp="false" editable="false" enabled="true" visible="true" width="150px" valueHelpOnly="false"
										required="false" valueStateText="Invalid entry" maxLength="10"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Button text="Get Items" type="Emphasized" width="150px" enabled="true" visible="true" iconDensityAware="false" press="getItems"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:fields>
									<VBox id="vboxTable" alignItems="Stretch" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
										displayInline="false">
										<items></items>
										<layoutData/>
									</VBox>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
				<content>
					<Button text="Save" type="Emphasized" width="auto" enabled="true" visible="true" iconDensityAware="false" press="save"/>
					<ToolbarSpacer width=""/>
				</content>
			</OverflowToolbar>
		</footer>
		<headerContent/>
		<subHeader/>
		<customHeader/>
	</Page>
</mvc:View>