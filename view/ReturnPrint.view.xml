<mvc:View class="sapUiSizeCozy" xmlns:mvc="sap.ui.core.mvc" controllerName="smud.org.wmLM01.controller.ReturnPrint" xmlns="sap.m"
	xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Page showHeader="true" title="Print Material Label" showFooter="true" showNavButton="false">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:title/>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Warehouse" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpLgnum" liveChange="uppercaseNoSpecial" value="{/Lgnum}" valueLiveUpdate="true" type="Text" showValueHelp="false"
										enabled="true" visible="true" width="75px" valueHelpOnly="false" required="true" valueStateText="Invalid entry" maxLength="3"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Material" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
										visible="true" displayInline="false">
										<items>
											<Input id="inpMatnr" value="{/Matnr}" type="Text" showValueHelp="false" enabled="true" change="getMaktg" visible="true" width="100%"
												valueHelpOnly="false" required="true" valueStateText="Invalid entry" maxLength="18"/>
											<Button id="btnMatnr" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true"
												iconDensityAware="false" press="scanMatnr"/>
										</items>
										<layoutData/>
									</HBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Description" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input value="{/Maktg}" valueLiveUpdate="true" type="Text" showValueHelp="false"
										enabled="true" visible="true" width="100%" editable="false" valueHelpOnly="false" valueStateText="Invalid entry" maxLength="40"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Qty" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpQty" value="{/Qty}" valueLiveUpdate="true" type="Tel" showValueHelp="false" enabled="true" visible="true" width="75px"
										liveChange="wholeNumber" valueHelpOnly="false" required="true" valueStateText="Invalid entry"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
				<content>
					<Button text="Print" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="print"/>
					<ToolbarSpacer width=""/>
				</content>
			</OverflowToolbar>
		</footer>
		<headerContent/>
		<subHeader/>
		<customHeader/>
	</Page>
</mvc:View>