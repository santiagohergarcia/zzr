<mvc:View class="sapUiSizeCozy" xmlns:mvc="sap.ui.core.mvc" controllerName="smud.org.wmLM01.controller.WmPrintBinLabel" xmlns="sap.m"
	xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Page showHeader="true" title="Print Bin Label" showFooter="true" showNavButton="false">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:title/>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Warehouse" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpLgnum" liveChange="uppercase" value="{/Lgnum}" valueLiveUpdate="true" type="Text" showValueHelp="false" enabled="true"
										visible="true" width="75px" valueHelpOnly="false" required="true" valueStateText="Invalid entry" maxLength="3"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Storage Type" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpLgtyp" liveChange="uppercase" value="{/Lgtyp}" valueLiveUpdate="true" type="Text" showValueHelp="false" enabled="true"
										visible="true" width="75px" valueHelpOnly="false" required="true" valueStateText="Invalid entry" maxLength="3"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Bin (from)" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
										visible="true" displayInline="false">
										<items>
											<Input id="inpLgplaFrom" value="{/LgplaFrom}" type="Text" showValueHelp="false" enabled="true" visible="true" width="250px"
												valueHelpOnly="false" required="true" valueStateText="Invalid entry" maxLength="10" liveChange="uppercase"/>
											<Button id="btnLgplaFrom" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true"
												iconDensityAware="false" press="scan"/>
										</items>
										<layoutData/>
									</HBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Bin (to)" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
										visible="true" displayInline="false">
										<items>
											<Input id="inpLgplaTo" value="{/LgplaTo}" type="Text" showValueHelp="false" enabled="true" visible="true" width="250px"
												valueHelpOnly="false" required="true" valueStateText="Invalid entry" maxLength="10" liveChange="uppercase"/>
											<Button id="btnLgplaTo" type="Default" icon="sap-icon://bar-code" iconFirst="true" width="50px" enabled="true" visible="true"
												iconDensityAware="false" press="scan"/>
										</items>
										<layoutData/>
									</HBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Qty" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="inpQty" value="{/Qty}" valueLiveUpdate="true" type="Tel" showValueHelp="false" liveChange="wholeNumber" enabled="true"
										visible="true" width="75px" valueHelpOnly="false" required="true" valueStateText="Invalid entry"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Label Size" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<RadioButtonGroup
										columns="1" selectedIndex="0" valueState="None" width="auto" editable="true" enabled="true" textDirection="Inherit">
										<buttons>
											<RadioButton text="4x2" editable="true" enabled="true" visible="true" width="auto" valueState="None" selected="{/Size4x2}"/>
											<RadioButton text="3x1" editable="true" enabled="true" visible="true" width="auto" valueState="None" selected="{/Size3x1}"/>
										</buttons>
									</RadioButtonGroup>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Label Type" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<RadioButtonGroup
										columns="1" selectedIndex="0" valueState="None" width="auto" editable="true" enabled="true" textDirection="Inherit">
										<buttons>
											<RadioButton text="Normal Label" editable="true" enabled="true" visible="true" width="auto" valueState="None" selected="{/NormalLabel}"/>
											<RadioButton text="Material Label" editable="true" enabled="true" visible="true" width="auto" valueState="None" selected="{/MaterialLabel}"/>
											<RadioButton text="Non Stock" editable="true" enabled="true" visible="true" width="auto" valueState="None" selected="{/NonStock}"/>
											<RadioButton text="South Dock" editable="true" enabled="true" visible="true" width="auto" valueState="None" selected="{/SouthDock}"/>
										</buttons>
									</RadioButtonGroup>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
				<content>
					<Button text="Print" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="print"/>
					<ToolbarSpacer width=""/>
				</content>
			</OverflowToolbar>
		</footer>
		<headerContent/>
		<subHeader/>
		<customHeader/>
	</Page>
</mvc:View>